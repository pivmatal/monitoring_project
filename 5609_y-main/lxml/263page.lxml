		<script>
		$(function() {

			$('#feedback').on('shown.bs.modal', function (e) {
				let trigger = $(e.relatedTarget);
				$('#feedbackLabel').html(trigger.data('type'));
				$(this).find("input[name=type]").val(trigger.data('type'));
			})

			$('#exampleModal,#itemModal2,#itemModal3,#itemModal4').on('shown.bs.modal', function (e) {
				let trigger = $(e.relatedTarget);
				$(this).find("input[name=type]").val(trigger.data('type'));
			})

		});

		</script>

		<script>

		$(".recall").submit(function() {
			//$(this).find(':submit').attr('disabled','disabled');
			var thisform = $(this);
			$('*[type="submit"]').addClass("loading");
			var str = $(this).serialize();
			var stop=0;

			if(thisform.find($('input[name="phone"]')).val().length <= 0){
				thisform.find($('input[name="phone"]')).addClass('redphone');
				stop=1;
			}else{
				thisform.find($('input[name="phone"]')).removeClass('redphone');
			}

			if(thisform.find($('input[name="name"]')).val().length <= 0){
				thisform.find($('input[name="name"]')).addClass('redphone');
				stop=1;
			}else{
				thisform.find($('input[name="name"]')).removeClass('redphone');
			}


			if(stop==0){
				$.ajax({
					type: "POST",
					async: false,
					url: thisform.attr('action'),
					data: str,
					success: function(msg) {

						thisform.trigger('reset');

						if(msg == "OK") {
							result = '<div class="alert alert-success mb-0 mt-2 py-2"><span>Заявка успешно отправлена.</span> Мы свяжемся с Вами!</div>';
							$("input", "form").not(":button, :submit, :reset, :hidden").val("");
							thisform.find(".form_message").html(result);
						}
						else {result = msg;thisform.find(".form_message").html('<div class="alert alert-danger mb-0 mt-2 py-2">'+result+'</div>');$('*[type="submit"]').removeClass("loading");}
						//$(this).find(':submit').attr('disabled','disabled');
					}
				});
			}else{
				thisform.find(".form_message").html('<div class="alert alert-danger mb-0 mt-2 py-2">Укажите Ваше имя и номер телефона.</div>');$('*[type="submit"]').removeClass("loading");
			}
			return false;
		});

		</script>


